if (stmt.type == "function_call"):
            context, return_type, line_number, parameters, statements = self.ftable.call(stmt.identifier, stmt.arguments)
            func_state = State(statements)
            # func_state.scope = copy.deepcopy(self.scope)
            func_state.ftable = copy.deepcopy(self.ftable)
            func_state.heap = self.heap
            
            for i in range(len(self.statements)):
                func_state = func_state.next()
            
            self.heap = func_state.heap
            self.ip += 1